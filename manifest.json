{
	"manifest_version" : 2,
	"name" : "SafeBrowsing",
	"version" : "0.1",
	"browser_action" : {
		"default_icon" : "icon.png"
	},
	"content_scripts" : [
		{
			"matches" : ["http://*/*", "https://*/*"],
			"js" : ["main.js", "tests/tic_tac_toe.js"],
			"css" : ["tests/type.css"]
		}
	],
	"web_accessible_resources" : ["tests/*"],
	"permissions" : [
		"storage"
	]
}